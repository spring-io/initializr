<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
		"-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
		"https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
	<suppress files=".+Application\.java" checks="HideUtilityClassConstructor"/>
	<suppress files="[\\/]initializr-service-sample[\\/]" checks="JavadocType"/>
	<suppress files="[\\/]initializr-service-sample[\\/]" checks="ImportControl"/>
	<suppress files="MavenBuildSettings\.java" checks="NoWhitespaceBefore" message="'...'"/>
	<suppress files="AbstractInitializrIntegrationTests\.java" checks="NoWhitespaceBefore" message="'...'"/>
</suppressions>
